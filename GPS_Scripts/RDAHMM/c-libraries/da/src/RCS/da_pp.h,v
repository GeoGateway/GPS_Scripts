@new format.
@
text
@/*******************************************************************************
BLANKET HEADER:
da_pp.h
*******************************************************************************/

#ifndef _DA_PP_H_
#define _DA_PP_H_
/* Protects from multiple inclusion. */

#ifndef lint
static char da_pp_h_rcsid[] = "$Id: da_pp.h,v 1.4 1996/10/31 02:17:39 agray Exp agray $";
#endif
/* This string variable allows the RCS identification info to be printed. */

